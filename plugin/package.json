{
    "name": "@badideas/nativescript-push",
    "version": "1.2.1",
    "description": "Very simple push notification plugin",
    "main": "./messaging",
    "typings": "./messaging.d.ts",
    "sideEffects": false,
    "nativescript": {
        "platforms": {
            "android": "5.0.0",
            "ios": "5.0.0"
        },
        "hooks": [
            {
                "type": "after-prepare",
                "script": "scripts/after-prepare.js",
                "inject": true
            }
        ]
    },
    "dependencies": {
        "@nativescript/hook": "~2.0.0"
    },
    "scripts": {
        "preuninstall": "node preuninstall.js",
        "postinstall": "node postinstall.js"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Tyler Flint",
        "email": "tylerflint@gmail.com"
    },
    "bugs": {
        "url": "https://github.com/tylerflint/nativescript-push/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/tylerflint/nativescript-push",
    "readmeFilename": "README.md"
}
